<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>

    <bean id="cryptodoc.license" class="org.alfresco.enterprise.repo.content.cryptodoc.impl.CryptodocLicenseImpl" >
    </bean> 

    <!-- Encrypted ContentStore subsystem -->
    <bean id="encryptedContentStore" class="org.alfresco.repo.management.subsystems.ChildApplicationContextFactory" parent="abstractPropertyBackedBean">
        <property name="autoStart">
            <value>false</value>
        </property>
        <property name="category">
            <value>ContentStore</value>
        </property>
        <property name="typeName">
            <value>encrypted</value>
        </property>
        <property name="instancePath">
            <list>
                <value>managed</value>
                <value>encrypted</value>
            </list>
        </property>
    </bean>

</beans>