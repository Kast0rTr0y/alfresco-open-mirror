<webscript>
  <shortname>cloud-sync-conflict</shortname>
  <description>Marks a node locally as being in conflict</description>
  <url>/enterprise/sync/conflict</url>
  <format default="json">argument</format>
  <authentication>user</authentication>
  <lifecycle>internal</lifecycle>

  <!-- The services we call handle the transaction stuff carefully. -->
  <!-- We need to disable transactions, so we can get the raw request -->
  <transaction>none</transaction>

  <!-- We may do special multi-part handling -->
  <formdata multipart-processing="false" />
</webscript>
