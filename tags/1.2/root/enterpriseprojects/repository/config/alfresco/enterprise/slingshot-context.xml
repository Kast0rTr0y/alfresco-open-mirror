<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">

    <bean id="doclibCustomResponseAos" class="org.alfresco.enterprise.repo.jscript.app.AosCustomResponse">
        <property name="sysAdminParams">
            <ref bean="sysAdminParams" />
        </property>
        <property name="baseUrlOverwrite">
            <value>${aos.baseUrlOverwrite}</value>
        </property>
    </bean>
    
    <!-- overwrite community CustomResponse map -->
    <util:map id="slingshotDocLibCustomResponsesMap">
        <entry key="aos">
            <ref bean="doclibCustomResponseAos"/>
        </entry>
        <!-- We still configure the vtiServer custom response so that the legacy vti module is still supported -->
        <entry key="vtiServer">
            <ref bean="doclibCustomVtiServer"/>
        </entry>
    </util:map>

</beans>
