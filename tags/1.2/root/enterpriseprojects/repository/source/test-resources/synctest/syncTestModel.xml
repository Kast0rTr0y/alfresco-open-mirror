<?xml version="1.0" encoding="UTF-8"?>
<model name="foosync:model" xmlns="http://www.alfresco.org/model/dictionary/1.0">
    <description>Sync Test Dynamic Custom Model</description>
    <author>mrogers</author>
    <version>1.0</version>
    <imports>
        <import prefix="d" uri="http://www.alfresco.org/model/dictionary/1.0"/>
        <import prefix="cm" uri="http://www.alfresco.org/model/content/1.0"/>
    </imports>
    <namespaces>
        <namespace prefix="foosync" uri="http://www.alfresco.org/syncTest/foosync/1.0"/>
    </namespaces>
    <types>
        <type name="foosync:foofolder">
            <title>Foo Sync folder</title>
            <parent>cm:folder</parent>
            <properties>
                <property name="foosync:testText">
                    <title>Foo Bar</title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                </property>
                
                <property name="foosync:testFloat">
                    <title>Foo Test Float</title>
                    <type>d:float</type>
                    <mandatory>false</mandatory>
                </property>
                
            </properties>
        </type>
		<type name="foosync:fooContent">
            <title>Foo Sync Content</title>
            <parent>cm:content</parent>
            <properties>
                <property name="foosync:testContentText">
                    <title>Foo Sync Content Test Text</title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                </property>
                
                <property name="foosync:testContentFloat">
                    <title>Foo Sync Content Test Float</title>
                    <type>d:float</type>
                    <mandatory>false</mandatory>
                </property>
                
            </properties>
        </type>
    </types>
    <aspects>
        <aspect name="foosync:testMarkerAspect" />
        <aspect name="foosync:testAspect" >
            <title>Sync Test Aspect</title>
            <properties>
                <property name="foosync:testAspectProperty">
                    <title>Test Aspect Property</title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                </property>
            </properties>
        </aspect>
    </aspects>
</model>
