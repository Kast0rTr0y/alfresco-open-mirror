<?xml version='1.0' encoding='UTF-8'?>
<page>
   <title>Cloud Account</title>
   <description>Page for rejecting/completing signup of accounts</description>
   <template-instance>simple-guest</template-instance>
   <authentication>guest</authentication>
   <components>

      <!-- Signup profile -->
      <component>
         <region-id>components</region-id>
         <url>/cloud/core/components/account/signup-completion</url>
         <properties>
            <key>{key}</key>
            <pid>{id}</pid>
            <cancel>{cancel}</cancel>
         </properties>
      </component>

   </components>
</page>