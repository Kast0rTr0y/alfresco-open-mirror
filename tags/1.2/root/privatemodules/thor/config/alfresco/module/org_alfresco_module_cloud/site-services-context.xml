<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>
    
    <!-- Override the core siteService bean -->
    <bean id="siteService" parent="baseSiteService" class="org.alfresco.module.org_alfresco_module_cloud.site.CloudSiteServiceImpl">
        <property name="directoryService" ref="directoryService"/>
        <property name="accountService" ref="AccountService"/>
        <property name="cloudPersonService" ref="cloudPersonService"/>
    </bean>
    
</beans>
